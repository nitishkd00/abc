{
  "name": "pulasa-auction",
  "version": "1.0.0",
  "description": "Centralized real-time auction system for rare fish (Pulasa) with INR payments",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "auction",
    "fish",
    "pulasa",
    "razorpay",
    "real-time",
    "react",
    "nodejs",
    "mysql",
    "socketio"
  ],
  "author": "Pulasa Auction Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "history": "^5.3.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  }
}
